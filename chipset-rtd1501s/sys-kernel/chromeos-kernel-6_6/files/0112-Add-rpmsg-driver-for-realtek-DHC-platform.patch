From da978656f9460e950dce7cef61332c257c0ff9cb Mon Sep 17 00:00:00 2001
From: TY Chang <tychang@realtek.com>
Date: Thu, 8 Dec 2022 16:41:25 +0800
Subject: [PATCH] rpmsg: add rpmsg driver for realtek DHC platform

Change-Id: I685230c74adc19bb57a2cefa7a19c0f3442c68f9
---
 drivers/rpmsg/Kconfig           |    4 +
 drivers/rpmsg/Makefile          |    1 +
 2 files changed, 1496 insertions(+)

--- a/drivers/rpmsg/Kconfig
+++ b/drivers/rpmsg/Kconfig
@@ -81,4 +81,9 @@
 	select RPMSG_NS
 	select VIRTIO
 
+config RPMSG_RTK_RPC
+	tristate "RTK RPC DRIVER"
+	depends on RTK_FW_REMOTEPROC
+	select RPMSG
+
 endmenu
--- a/drivers/rpmsg/Makefile
+++ b/drivers/rpmsg/Makefile
@@ -10,3 +10,4 @@
 obj-$(CONFIG_RPMSG_QCOM_GLINK_SMEM) += qcom_glink_smem.o
 obj-$(CONFIG_RPMSG_QCOM_SMD)	+= qcom_smd.o
 obj-$(CONFIG_RPMSG_VIRTIO)	+= virtio_rpmsg_bus.o
+obj-$(CONFIG_RPMSG_RTK_RPC) += rpmsg_rtk.o
