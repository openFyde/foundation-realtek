From ff6964fb368edb74575fa15887f45dc6cc86be30 Mon Sep 17 00:00:00 2001
From: yh_hsieh <yh_hsieh@realtek.com>
Date: Mon, 15 Jan 2024 15:51:46 +0800
Subject: [PATCH] enable rtk rproc driver

---
 drivers/remoteproc/Kconfig  | 5 +++++
 drivers/remoteproc/Makefile | 1 +
 2 files changed, 6 insertions(+)

--- a/drivers/remoteproc/Kconfig
+++ b/drivers/remoteproc/Kconfig
@@ -365,6 +365,11 @@
 
 	  It's safe to say N if not interested in using RPU r5f cores.
 
+config RTK_FW_REMOTEPROC
+	tristate "RTK REMOTEPROC DRIVER"
+	help
+	 enable RTK's rproc dirver.
+
 endif # REMOTEPROC
 
 endmenu
--- a/drivers/remoteproc/Makefile
+++ b/drivers/remoteproc/Makefile
@@ -39,3 +39,4 @@
 obj-$(CONFIG_TI_K3_DSP_REMOTEPROC)	+= ti_k3_dsp_remoteproc.o
 obj-$(CONFIG_TI_K3_R5_REMOTEPROC)	+= ti_k3_r5_remoteproc.o
 obj-$(CONFIG_XLNX_R5_REMOTEPROC)	+= xlnx_r5_remoteproc.o
+obj-$(CONFIG_RTK_FW_REMOTEPROC)		+= rtk_fw_remoteproc.o
