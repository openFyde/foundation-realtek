bootargs=earlycon=uart8250,mmio32,0x98007800 console=ttyS0,460800 uio_pdrv_genirq.of_id=generic-uio init=/sbin/init root=/dev/mmcblk1p2 rw rootfstype=ext4 rootwait mtdparts=RtkSFC:256k(u-boot-env),384k(u-boot-spl)ro,384k(u-boot) pd_ignore_unused clk_ignore_unused video=HDMI-A-1:1920x1080@30 console=tty1 loglevel=4
bootcmd=fatload mmc 1:1 ${bootmaddr} yocto.itb; bootm ${bootmaddr}${bootcfg}
altbootcmd=env set bootcfg "#rescue"; run bootcmd
bootdelay=2
initrd_high=0xffffffffffffffff
fdt_high=0xffffffffffffffff
loadaddr=0x03000000
bootmaddr=0x08000000
baudrate=460800
eth_drv_para=rgmii2
ethact=r8168#0
ethaddr=00:10:20:30:40:50
gatewayip=192.168.100.254
ipaddr=192.168.100.1
netmask=255.255.255.0
serverip=192.168.100.2
