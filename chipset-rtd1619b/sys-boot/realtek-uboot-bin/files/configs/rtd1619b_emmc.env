bootargs=earlycon=uart8250,mmio32,0x98007800 console=ttyS0,460800 uio_pdrv_genirq.of_id=generic-uio init=/sbin/init root=/dev/mmcblk0p2 rw rootfstype=ext4 rootwait firmware_class.path=/lib/firmware/realtek/rtd1619b/ pd_ignore_unused clk_ignore_unused loglevel=4
boot_yacto=fatload mmc 0:1 ${bootmaddr} yocto.itb; bootm ${bootmaddr}
altbootcmd=fatload mmc 0:1 ${bootmaddr} yocto.itb; bootm ${bootmaddr}#rescue
bootdelay=2
initrd_high=0xffffffffffffffff
fdt_high=0xffffffffffffffff
loadaddr=0x03000000
bootmaddr=0x08000000
baudrate=460800
ethact=r8168#0
ethaddr=00:10:20:30:40:50
gatewayip=192.168.100.254
ipaddr=192.168.100.1
netmask=255.255.255.0
serverip=192.168.100.2
script_img=/boot/boot.scr.uimg
script_addr=0x7000000
distro_bootpart=c
devnum=0
find_uimg=setenv dev_type; for dev_type in mmc sd usb; do if test -e ${dev_type} ${devnum}:${distro_bootpart} ${script_img}; then setenv devtype ${dev_type}; fi; done
setup_usb=if env exists boot_usb; then usb start; fi
boot_chromium_os=run setup_usb; run find_uimg; load ${devtype} ${devnum}:${distro_bootpart} ${script_addr} ${script_img}; source ${script_addr}
bootcmd=run boot_chromium_os; run boot_yacto
