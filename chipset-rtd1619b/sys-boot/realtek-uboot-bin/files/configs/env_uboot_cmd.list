env set boot_yacto "fatload mmc 0:1 ${bootmaddr} yocto.itb; bootm ${bootmaddr}"
env set script_img /boot/boot.scr.uimg
env set script_addr 0x7000000
env set distro_bootpart c
env set devnum 0
env set find_uimg "setenv dev_type; for dev_type in mmc sd usb; do if test -e ${dev_type} ${devnum}:${distro_bootpart} ${script_img}; then setenv devtype ${dev_type}; fi; done"
env set setup_usb "if env exists boot_usb; then usb start; fi"
env set boot_chromium_os "run setup_usb; run find_uimg; load ${devtype} ${devnum}:${distro_bootpart} ${script_addr} ${script_img}; source ${script_addr}"
env set bootcmd "run boot_chromium_os; run boot_yacto"
env save
