From 22aa4a44a491ef20ac3d06b1d9a7a6665fae6444 Mon Sep 17 00:00:00 2001
From: James Tai <james.tai@realtek.com>
Date: Sun, 13 Nov 2022 20:38:26 +0800
Subject: [PATCH 15/69] mtd: add mfd driver for apw888x series PMIC

Change-Id: Id1806c06511eca150c49344c87a135969842708d
---
 drivers/mfd/Kconfig  | 11 +++++++++++
 drivers/mfd/Makefile |  2 ++
 2 files changed, 13 insertions(+)

diff --git a/drivers/mfd/Kconfig b/drivers/mfd/Kconfig
index 064299ea2..9d28180f8 100644
--- a/drivers/mfd/Kconfig
+++ b/drivers/mfd/Kconfig
@@ -2220,5 +2220,16 @@ config MFD_RSMU_SPI
 	  Additional drivers must be enabled in order to use the functionality
 	  of the device.
 
+config MFD_APW8889_I2C
+	tristate "Support for Anpec-APW8889 PMIC"
+	depends on I2C
+	select MFD_CORE
+	select REGMAP_I2C
+
+config MFD_APW8886_I2C
+	tristate "Support for Anpec-APW8886 PMIC"
+	depends on I2C
+	select MFD_CORE
+	select REGMAP_I2C
 endmenu
 endif
diff --git a/drivers/mfd/Makefile b/drivers/mfd/Makefile
index e0db12a24..912a33f00 100644
--- a/drivers/mfd/Makefile
+++ b/drivers/mfd/Makefile
@@ -252,6 +252,8 @@ obj-$(CONFIG_MFD_ALTERA_SYSMGR) += altera-sysmgr.o
 obj-$(CONFIG_MFD_STPMIC1)	+= stpmic1.o
 obj-$(CONFIG_MFD_SUN4I_GPADC)	+= sun4i-gpadc.o
 
+obj-$(CONFIG_MFD_APW8889_I2C) += apw8889-i2c.o
+obj-$(CONFIG_MFD_APW8886_I2C) += apw8886-i2c.o
 obj-$(CONFIG_MFD_STM32_LPTIMER)	+= stm32-lptimer.o
 obj-$(CONFIG_MFD_STM32_TIMERS) 	+= stm32-timers.o
 obj-$(CONFIG_MFD_MXS_LRADC)     += mxs-lradc.o
-- 
